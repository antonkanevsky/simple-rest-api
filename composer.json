{
    "name": "antonkanevsky/simple-rest-api",
    "description": "Simple REST API example",
    "keywords": ["REST", "Symfony components"],
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "symfony/http-foundation": "^4.4",
        "symfony/routing": "^4.4",
        "symfony/yaml": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/config": "^4.4",
        "symfony/expression-language": "^4.4",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
